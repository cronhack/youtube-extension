{
  "manifest_version": 3,
  "name": "YouTube Ad Skipper",
  "version": "1.0",
  "description": "Skips YouTube ads automatically.",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["content.js"]
    }
  ]
}
